<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ButtonProgress">
        <attr name="progress_button_title" format="string"/>
        <attr name="progress_button_loading_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="ProgressButton">
        <attr name="size" format="string"/>
    </declare-styleable>

    <declare-styleable name="HorizontalLoading">
        <attr name="numberDots" format="integer" />
        <attr name="fadeTime" format="integer" />
        <attr name="radius" format="integer" />
        <attr name="color" format="color"/>
        <attr name="dotRadius" format="integer" />

    </declare-styleable>


</resources>